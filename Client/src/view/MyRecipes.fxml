<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MyRecipesViewController">
   <children>
      <TableView fx:id="myRecipeList" layoutX="282.0" layoutY="63.0" prefHeight="200.0" prefWidth="266.0">
        <columns>
          <TableColumn fx:id="categoryColumn" prefWidth="75.0" text="Category" />
          <TableColumn fx:id="recipeColumn" prefWidth="75.0" text="Recipe" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="116.0" layoutY="81.0" mnemonicParsing="false" onAction="#onAddRecipe" text="Add Recipe" />
      <Button layoutX="116.0" layoutY="137.0" mnemonicParsing="false" onAction="#onEditRecipe" text="Edit Recipe" />
      <Button layoutX="109.0" layoutY="200.0" mnemonicParsing="false" onAction="#removeRecipeButtonPressed" text="Delete Recipe" />
      <Button layoutX="522.0" layoutY="360.0" mnemonicParsing="false" text="Log out" />
      <Button layoutX="57.0" layoutY="348.0" mnemonicParsing="false" onAction="#backToAllRecipes" text="Back to all recipes" />
      <Label fx:id="deleteErrorLabel" layoutX="281.0" layoutY="274.0" prefHeight="18.0" prefWidth="266.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
